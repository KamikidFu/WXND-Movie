.wrapper{
  position: absolute;
  width: 100%;
  height: 100%;
  margin-bottom: 0;
  background-color: rgb(240, 238, 238);
}

.footer{
  position: fixed;
  bottom: 0;
  display: flex;
  flex-direction: row;
  border-top: solid 1px rgb(240, 238, 238);
  width: 750rpx;
  background-color: #FFFFFF;
}

.footerFunction{
  text-align: center;
  width: 750rpx; 
  cursor: pointer;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  height: 100rpx;
  font-size: 35rpx;
  line-height: 49rpx;
  color: black;
}
